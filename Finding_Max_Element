package com.ts.array;
/*
 * find max element in the givin arrays 
 */

public class Finding_Max_Element {

	public static void main(String[] args) {

		int ar[] = { 1, 2, 3, 4, 5 };
		int max = findMax(ar, 5,);
		System.out.println("max element is:: " + max);

	}

	private static int findMax(int[] ar, int n) {
		int max = 0;
		for (int i = 0; i < n; i++) {
			if (max < ar[i]) {
				max = ar[i];
			}
		}
		return max;
	}

}
