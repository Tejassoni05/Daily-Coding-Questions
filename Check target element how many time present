// count the k= 11 how many time it is there in array 
class Main {
    public static void main(String[] args) {
        
        int [] arr = {11,45,10,11,88,66, 11};
        int count = 0 ; 
        int target = 11;
        for(int num : arr){
            if(num == target){
                count++ ;
            }
        }
        System.out.println("target element is present: "+ count + " times");
    }
}
