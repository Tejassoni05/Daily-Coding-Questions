// find the factorial of number 
class Main {
    public static long findFactorial(int num){
      int factorial = 1 ; 
      for(int i = 1 ; i <= num ; i++ ){
          factorial = factorial * i ; 
      }
      return factorial; 
        
    }
    public static void main(String[] args) {
    
        
        
        System.out.println(findFactorial(3));
    }
}
