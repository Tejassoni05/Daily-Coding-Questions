package com.ts.pattern;

import java.util.Scanner;

public class BinaryPattern1 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		boolean flag = true;
		for (int i = 1; i <= n; i++) {
			for (int j = 1; j <= n; j++) {
				if (flag == true) {
					System.out.print(1);
				flag = false;
				} else {
					System.out.print(0);
					flag = true;
				}
			}
			System.out.println();
		}

	}

}
