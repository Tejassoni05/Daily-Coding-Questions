class Main {
    public static void main(String[] args) {
        String str = "Tejas" ;
        int strLength = str.length();
        String rev = ""; 
        str.rev();
        for(int i = strLength-1 ; i >= 0 ; i--){
             rev = rev + str.charAt(i); 
        }
    System.out.println("Try programiz.pro  "+ rev);
        System.out.println("Try programiz.pro"+ strLength);
        
    }
}
