class Main {
    public static void main(String[] args) {
        String str = "121222";
        String removeDup= "";
        for(int i = 0 ; i < str.length()-1; i++){
            if(str.charAt(i)!= str.charAt(i+1)){
                 removeDup =  removeDup+str.charAt(i);
            }
        }
        System.out.println(removeDup);
    }
}
