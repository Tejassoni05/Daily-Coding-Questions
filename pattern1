package com.ts.pattern;

import java.util.Scanner;
/*   *****  row is = n.
 *   *****
 *   *****   */ // r=n*2-1

public class pattern1 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter n = ");
		int n = sc.nextInt();
		for (int r = 1; r <= n; r++) {
			for (int c = 1; c <= n; c++) {
				System.out.print(c);
			}
			System.out.println();
		}

	}

}
