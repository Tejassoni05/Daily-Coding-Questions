package com.ts.pattern;

import java.util.Scanner;
// 5432*
// 543*1
// 54*21 
// 5*321
// *4321
public class Pattern10 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		for (int i = 1; i <= n; i++) {
			for (int j = 5; j >= 1; j--) {
				if (j == i) {
					System.out.print("*");
				} else {
					System.out.print(j);
				}
			}
			System.out.println();
		}
		sc.close();
	}
}
